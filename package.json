{
	"name": "autoinclude",
	"displayName": "auto Include",
	"publisher": "homeboy445",
	"description": "Automatically include & remove C++ headers accordingly.",
	"repository": {
		"url": "https://github.com/homeboy445/auto-Include"
	},
	"icon":"icon.png",
	"version": "1.0.1",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [
		"onCommand:autoinclude.autoInclude"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "autoinclude.autoInclude",
				"title": "auto Include"
			}
		]
	},
	"keywords": [
		"C++",
		"cpp"
	],
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.61.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
