{
  "name": "autoinclude",
	"displayName": "autoInclude",
	"description": "Automatically include the headers of C++ accordingly.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:autoinclude.autoInclude"
	],
	"main": "./src/extension.js",
	"contributes": {
		"commands": [{
            "command": "autoinclude.autoInclude",
            "title": "auto Include"
		}],
		"keybindings": [
            {
                "command": "autoinclude.autoInclude",
                "key": "ctrl+a+l",
                "mac": "shift+a+l"
            }
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.61.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.2.2",
        "@types/node": "14.x",
        "eslint": "^7.27.0",
        "glob": "^7.1.7",
        "mocha": "^8.4.0",
        "typescript": "^4.3.2",
        "vscode-test": "^1.5.2"
    }
}
